{
    "main_select": {
        "type":"select",
        "nick": {
            "modal": {
                "title": "暱稱格式設定",
                "custom_id": "nick_format_setting",
                "input_text": [
                    {
                        "style": "short",
                        "label": "暱稱開頭的預設值",
                        "placeholder": "請輸入指定的預設格式",
                        "custom_id": "start"
                    },
                    {
                        "style": "short",
                        "label": "暱稱結尾的預設值",
                        "placeholder": "輸入指定的預設格式",
                        "custom_id": "end"
                    }
                ]
            }
        },
        "words": {
            "modal": {
                "title": "文字設定",
                "custom_id": "words_modal_setting",
                "input_text": [
                    {
                        "style": "short",
                        "label": "新增禁用的文字",
                        "placeholder": "輸入文字",
                        "custom_id": "add"
                    },
                    {
                        "style": "short",
                        "label": "解除禁用的文字",
                        "placeholder": "輸入文字",
                        "custom_id": "remove"
                    }
                ]
            }
        },
        "roles": {
            "ephemeral":true,
            "embed": {
                "title": "黑名單設定",
                "description": "請透過下方按鈕來進行操作",
                "fields":[]
            },
            "view": {
                "timeout":null,
                "buttons": [
                    {
                        "row": null,
                        "style": "primary",
                        "label": "新增身分組",
                        "custom_id": "add_block_roles_setting"
                    },
                    {
                        "row": null,
                        "style": "danger",
                        "label": "移除身分組",
                        "custom_id": "remove_block_roles_setting"
                    },
                    {
                        "row": 1,
                        "style": "primary",
                        "label": "新增用戶",
                        "custom_id": "add_block_user_setting"
                    },
                    {
                        "row": 1,
                        "style": "danger",
                        "label": "移除用戶",
                        "custom_id": "remove_block_user_setting"
                    }
                ],
                "selects": []
            }
        },
        "other":{}
    },
    "add_block_roles": {
        "ephemeral":true,
        "embed": {
            "title": "新增黑名單身分組",
            "description": "請從以下選單選擇身分組",
            "fields":[]
        },
        "view": {
            "timeout": null,
            "buttons": [],
            "selects": [
                {
                    "type": "role",
                    "placeholder": "選擇身分組",
                    "max_values": 25,
                    "custom_id": "add_block_roles_select_setting"
                }
            ]
        }
    },
    "remove_block_roles": {
        "ephemeral":true,
        "embed": {
            "title": "移除黑名單身分組",
            "description": "請從以下選單選擇身分組",
            "fields":[]
        },
        "view": {
            "timeout": null,
            "buttons": [],
            "selects": [
                {
                    "type": "role",
                    "placeholder": "選擇身分組",
                    "max_values": 25,
                    "custom_id": "remove_block_roles_select_setting"
                }
            ]
        }
    },
    "add_block_user": {
        "ephemeral":true,
        "embed": {
            "title": "新增黑名單成員",
            "description": "請從以下選單選擇身分組",
            "fields":[]
        },
        "view": {
            "timeout": null,
            "buttons": [],
            "selects": [
                {
                    "type": "role",
                    "placeholder": "選擇身分組",
                    "max_values": 25,
                    "custom_id": "add_block_user_select_setting"
                }
            ]
        }
    },
    "remove_block_user": {
        "ephemeral":true,
        "embed": {
            "title": "移除黑名單成員",
            "description": "請從以下選單選擇身分組",
            "fields":[]
        },
        "view": {
            "timeout": null,
            "buttons": [],
            "selects": [
                {
                    "type": "user",
                    "placeholder": "選擇身分組",
                    "max_values": 25,
                    "custom_id": "remove_block_user_select_setting"
                }
            ]
        }
